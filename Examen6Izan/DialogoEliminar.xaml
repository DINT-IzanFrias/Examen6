<Window x:Class="Examen6Izan.DialogoEliminar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Examen6Izan"
        mc:Ignorable="d"
        Title="DialogoEliminar" Height="500" Width="400">
    <StackPanel>
        <Image Source="{Binding Path=Deseo.Imagen}"></Image>
        <StackPanel Orientation="Horizontal">
            <Button Margin="15" Content="Eliminar" MouseLeftButtonUp="Eliminar"></Button>
            <Button Margin="15" Content="Cancelar"></Button>
        </StackPanel>    
    </StackPanel>
</Window>
